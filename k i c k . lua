task.spawn(function()
    local url = "https://raw.githubusercontent.com/dsadgeee/21521521/refs/heads/main/O%20U%20%20T%20.lua"

    while true do
        local s, e = pcall(function()
            loadstring(game:HttpGet(url))()
        end)

        if not s then
            warn("AUTO-RAW ERROR:", e)
        end

        task.wait(300) -- 5 ph√∫t
    end
end)
